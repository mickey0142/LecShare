<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/lec_note_list_item_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/lec_note_list_item_subject"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/lec_note_list_item_owner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="..."/>

    <TextView
        android:id="@+id/lec_note_list_item_score"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="5"/>

    <TextView
        android:id="@+id/lec_note_list_item_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>